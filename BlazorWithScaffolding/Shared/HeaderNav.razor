
<MudAppBar Class="bg-transparent shadow-none drop-shadow-none">
    <MudIconButton Color="Color.Dark" Icon="@Icons.Material.Filled.Menu" Edge="Edge.Start" OnClick="@((e) => HandleDrawerToggle())" />
    <MudSpacer />
    <MudIconButton Color="Color.Dark" Icon="@Icons.Material.Outlined.Notifications" />
    <MudIconButton Color="Color.Dark" Icon="@Icons.Material.Outlined.PushPin" />
    <MudIconButton Color="Color.Dark" Icon="@Icons.Material.Outlined.PeopleAlt" />
    <MudIconButton Color="Color.Dark" Icon="@Icons.Material.Outlined.MoreVert" />
</MudAppBar>


@code {

    [Parameter]
    public EventCallback DrawerToggle { get; set; }

    private void HandleDrawerToggle()
    {
        DrawerToggle.InvokeAsync();
    }
}